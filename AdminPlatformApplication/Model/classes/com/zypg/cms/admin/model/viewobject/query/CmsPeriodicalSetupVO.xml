  <SQLQuery><![CDATA[SELECT c.category_id,
       c.category_name,
       r.lib_code,
       r.lib_type_code,
       r.comp_code,
       s.periodical_type,
       lookup1.meaning periodical_type_name,
       s.paper_type,
       lookup2.meaning paper_type_name,
       s.chief_editor,
       s.price,
       s.book_size,
       s.cn,
       s.issn,
       s.words,
       s.prod_size,
       s.reader_group,
       s.org,
       s.publishing,
       s.description,
       s.total,
       s.lang,
       s.attribute1,
       cms_admin_platform_pkg.get_periodical_published_date(c.category_id,'PERIODICAL') published_date,
       s.attribute2
  FROM cms_category_t           c,
       cms_lib_category_g_rel_t r,
       cms_periodical_setup_t   s,
       cms_lookup_value_t       lookup1,
       cms_lookup_value_t       lookup2
 WHERE cms_common_pkg.get_category_top_node_id(c.category_id) = r.rel_id
   AND c.category_id = s.periodical_category_id
   AND r.lib_code = 'PERIODICAL'
   AND s.periodical_type = lookup1.lookup_value_code
   AND lookup1.lookup_type_code = 'PERIODICAL_TYPE'
   AND s.paper_type = lookup2.lookup_value_code
   AND lookup2.lookup_type_code = 'PERIODICAL_PAPER_TYPE']]></SQLQuery>
