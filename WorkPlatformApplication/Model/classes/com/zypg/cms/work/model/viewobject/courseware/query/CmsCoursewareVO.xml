  <SQLQuery><![CDATA[SELECT t.courseware_id,
       t.doc_id,
       t.courseware_name,
       t.teaching_material_name,
       t.teaching_material_isbn,
       t.teaching_material_author,
       t.upload_time,
       decode(sign(t.last_update_date - d.last_update_date),
              -1,
              d.last_update_date,
              t.last_update_date) last_update_date,
       d.status,
       d.lib_code,
       d.comp_code,
       NULL checked,
       dcr.category_id,
       dcr.category_group_type,
       nvl(d.delete_flag, 'N') delete_flag,
       d.attribute1 sync_flag,
       t.teaching_material_editor,
       t.teaching_material_series_name,
       t.courseware_type,
       t.copyright_flag,
       t.keyword,
       t.remarks
  FROM cms_courseware_t t, cms_doc_t d, cms_doc_category_rel_t dcr
 WHERE t.doc_id = d.doc_id
   AND d.doc_id = dcr.doc_id
   AND (:bvCategoryId IS NULL OR
       dcr.category_id IN
       (SELECT c.category_id
           FROM cms_category_t c
          WHERE c.attribute5 LIKE
                '%' || (SELECT attribute5
                          FROM cms_category_t
                         WHERE category_id = :bvCategoryId) || '%') OR
       dcr.category_id = :bvCategoryId)]]></SQLQuery>
