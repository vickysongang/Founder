  <SQLQuery><![CDATA[SELECT cmselecprodteo.elec_prod_id,
       cmselecprodteo.elec_prod_name,
       cmselecprodteo.isbn,
       cmselecprodteo.item_code,
       cmselecprodteo.assort_book,
       cmselecprodteo.prod_type,
       cmselecprodteo.publishing_house,
       cmselecprodteo.lang,
       cmselecprodteo.carrier_type,
       cmselecprodteo.time_length,
       cmselecprodteo.amount,
       cmselecprodteo.disk_design,
       cmselecprodteo.editor,
       cmselecprodteo.text_editor,
       cmselecprodteo.producer,
       cmselecprodteo.pub_time,
       cmselecprodteo.keyword,
       cmselecprodteo.content_desc,
       cmselecprodteo.remarks,
       cmselecprodteo.doc_id,
       dcr.category_id,
       c.category_name,
       d.status,
       d.thumbnail_url,
       d.comp_code,
       NULL checked,
       comp.comp_name
  FROM cms_elec_prod_t        cmselecprodteo,
       cms_doc_t              d,
       cms_doc_category_rel_t dcr,
       cms_category_t         c,
       cms_comp_v             comp
 WHERE cmselecprodteo.doc_id = dcr.doc_id(+)
   AND cmselecprodteo.doc_id = d.doc_id
   AND dcr.category_group_type(+) = 'BY_CATEGORY'
   AND dcr.category_id = c.category_id(+)
   and d.status = 'PUBED_ELEC_PROD'
   and d.comp_code = comp.comp_code
   and d.comp_code = decode(:bvGroupUserFlag,'Y',d.comp_code,:bvCompCode)]]></SQLQuery>
