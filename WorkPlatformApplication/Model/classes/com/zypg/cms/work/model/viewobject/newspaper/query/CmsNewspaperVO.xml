  <SQLQuery><![CDATA[SELECT p.newspaper_id,
       p.doc_id,
       p.newspaper_category_id,
       p.editor,
       p.news_id,
       p.format_design,
       p.cover_design,
       p.pub_time,
       p.keyword,
       p.prod_size,
       p.color_num,
       p.cover_typeset,
       p.assort_prod,
       p.book_size,
       p.main_typeset,
       p.main_page,
       p.pagination,
       p.suggestion,
       p.remarks,
       d.status,
       d.lib_code,
       d.comp_code,
       NULL checked,
       dcr.category_id,
       dcr.category_group_type,
       NVL(d.DELETE_FLAG, 'N') delete_flag,
       d.THUMBNAIL_URL,
       ps.news_num,
       v.newspaper_category_name,
       v.newspaper_type,
       v.total,
       to_char(ps.YEAR) YEAR,
       d.attribute1 sync_flag,
       p.total_num,
       decode(sign(p.last_update_date - d.last_update_date),
              -1,
              d.last_update_date,
              p.last_update_date) last_update_date,
       p.typeset_comp
  FROM cms_newspaper_t          p,
       cms_doc_t                d,
       cms_doc_category_rel_t   dcr,
       cms_news_setup_t         ps,
       cms_newspaper_category_v v
 WHERE p.doc_id = d.doc_id
   AND d.doc_id = dcr.doc_id
   AND p.news_id = ps.setup_id(+)
   AND p.newspaper_category_id = v.newspaper_category_id(+)
   AND (:bvCategoryId IS NULL OR
       dcr.category_id IN
       (SELECT c.category_id
           FROM cms_category_t c
          WHERE c.attribute5 LIKE
                '%' || (SELECT attribute5
                          FROM cms_category_t
                         WHERE category_id = :bvCategoryId) || '%') OR
       dcr.category_id = :bvCategoryId)]]></SQLQuery>
