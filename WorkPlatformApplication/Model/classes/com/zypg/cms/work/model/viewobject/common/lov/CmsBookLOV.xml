      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery><![CDATA[SELECT b.BOOK_ID,
       b.DOC_ID,
       b.ISBN,
       b.SERIES,
       b.ITEM_CODE,
       b.LANG,
       b.EDITOR,
       b.FORMAT_DESIGN,
       b.COVER_DESIGN,
       b.AUTHOR,
       b.CHIEF_EDITOR,
       b.TRANSLATOR,
       b.PUB_TIME,
       b.IMPRESSION,
       b.EDITION,
       b.PAGINATION,
       b.PRICE,
       b.KEYWORD,
       b.READER_GROUP,
       b.SUGGESTION,
       b.PROD_SIZE,
       b.COLOR_NUM,
       b.COVER_TYPESET,
       b.ASSORT_PROD,
       b.BOOK_SIZE,
       b.MAIN_PAGE,
       b.MAIN_TYPESET,
       b.FORMAT_COMP,
       b.BOOK_NAME,
       b.PUBLISHING_HOUSE,
       d.lib_code,
       d.comp_code,
       comp.comp_name,
       d.status,
       dcr.category_id,
       c.category_name
  FROM cms_book_t             b,
       cms_doc_t              d,
       cms_doc_category_rel_t dcr,
       cms_category_t         c,
       cms_comp_v             comp
 WHERE b.doc_id = d.doc_id
   AND d.lib_code = 'BOOK'
   AND nvl(d.delete_flag, 'N') <> 'Y'
   AND d.doc_id = dcr.doc_id(+)
   AND dcr.category_id = c.category_id(+)
   AND d.comp_code = comp.comp_code(+)
   AND d.comp_code = :bvCompCode
   AND NOT EXISTS (SELECT 1
          FROM cms_author_copyright_t t
         WHERE t.book_doc_id = b.doc_id)]]></SQLQuery>
