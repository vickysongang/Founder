  <SQLQuery><![CDATA[SELECT a.audio_id,
       a.doc_id,
       a.file_name,
       a.file_format,
       a.file_size,
       a.file_time,
       a.file_store_time,
       decode(sign(a.last_update_date - d.last_update_date),
              -1,
              d.last_update_date,
              a.last_update_date) last_update_date,
       d.status,
       d.lib_code,
       NULL checked,
       dcr.category_id,
       dcr.category_group_type,
       NVL(d.delete_flag, 'N') delete_flag,
       a.keyword keyword,
       a.assort_book assort_book,
       a.preview_url,
       d.comp_code,
       a.series_name,
       a.isbn,
       a.item_code,
       a.carrier_type,
       a.lang,
       a.editor,
       a.text_editor,
       a.record_user,
       a.pub_time,
       a.content_desc,
       a.remarks
  FROM cms_audio_t a, cms_doc_t d, cms_doc_category_rel_t dcr
 WHERE a.doc_id = d.doc_id
   AND d.doc_id = dcr.doc_id
   AND (:bvCategoryId IS NULL OR
       dcr.category_id IN
       (SELECT c.category_id
           FROM cms_category_t c
          WHERE c.attribute5 LIKE
                '%' || (SELECT attribute5
                          FROM cms_category_t
                         WHERE category_id = :bvCategoryId) || '%') OR
       dcr.category_id = :bvCategoryId)]]></SQLQuery>
