  <SQLQuery><![CDATA[SELECT e.elec_prod_id,
       e.doc_id,
       e.elec_prod_name,
       e.isbn,
       decode(sign(e.last_update_date - d.last_update_date),
              -1,
              d.last_update_date,
              e.last_update_date) last_update_date,
       e.time_length,
       e.pub_time,
       e.assort_book,
       d.status,
       d.lib_code,
       NULL checked,
       dcr.category_id,
       dcr.category_group_type,
       NVL(d.delete_flag, 'N') delete_flag,
       d.thumbnail_url,
       e.keyword keyword,
       e.item_code item_code,
       d.comp_code,
       d.attribute1 sync_flag,
       e.publishing_house,
       e.lang,
       e.carrier_type,
       e.amount,
       e.disk_design,
       e.editor,
       e.text_editor,
       e.producer,
       e.content_desc,
       e.remarks
  FROM cms_elec_prod_t e, cms_doc_t d, cms_doc_category_rel_t dcr
 WHERE e.doc_id = d.doc_id
   AND d.doc_id = dcr.doc_id
   AND (:bvCategoryId IS NULL OR
       dcr.category_id IN
       (SELECT c.category_id
           FROM cms_category_t c
          WHERE c.attribute5 LIKE
                '%' || (SELECT attribute5
                          FROM cms_category_t
                         WHERE category_id = :bvCategoryId) || '%') OR
       dcr.category_id = :bvCategoryId)]]></SQLQuery>
