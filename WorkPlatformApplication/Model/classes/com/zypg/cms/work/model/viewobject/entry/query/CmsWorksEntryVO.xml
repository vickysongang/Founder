  <SQLQuery><![CDATA[SELECT we.entry_id,
       we.doc_id,
       we.title,
       we.subtitle,
       we.english_title,
       we.english_subtitle,
       we.SOURCE,
       we.author,
       we.translator,
       we.lang,
       we.word_count,
       we.writing_time,
       we.store_time,
       we.keyword,
       we.abstarct_desc,
       decode(sign(we.last_update_date - d.last_update_date),
              -1,
              d.last_update_date,
              we.last_update_date) last_update_date,
       d.STATUS,
       d.lib_code,
       NULL checked,
       dcr.CATEGORY_ID,
       dcr.CATEGORY_GROUP_TYPE,
       NVL(d.DELETE_FLAG, 'N') delete_flag,
       d.comp_code
  FROM cms_works_entry_t we, CMS_DOC_T d, CMS_DOC_CATEGORY_REL_T dcr
 WHERE we.DOC_ID = d.DOC_ID
   AND d.DOC_ID = dcr.DOC_ID
  AND (:bvCategoryId IS NULL OR
       dcr.category_id IN
       (SELECT c.category_id
                 FROM cms_category_t c
                WHERE c.attribute5 LIKE '%' ||
                      (SELECT attribute5
                         FROM cms_category_t
                        WHERE category_id = :bvCategoryId) || '%') OR
       dcr.category_id = :bvCategoryId)]]></SQLQuery>
