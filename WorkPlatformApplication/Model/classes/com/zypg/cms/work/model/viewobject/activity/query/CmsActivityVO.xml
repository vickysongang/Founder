  <SQLQuery><![CDATA[SELECT a.activity_id,
       a.doc_id,
       a.activity_theme,
       a.activity_location,
       a.activity_start_time,
       a.activity_end_time,
       decode(sign(a.last_update_date - d.last_update_date),
              -1,
              d.last_update_date,
              a.last_update_date) last_update_date,
       d.status,
       d.lib_code,
       d.comp_code,
       NULL checked,
       dcr.category_id,
       dcr.category_group_type,
       nvl(d.delete_flag, 'N') delete_flag,
       d.attribute1 sync_flag
  FROM cms_activity_t a, cms_doc_t d, cms_doc_category_rel_t dcr
 WHERE a.doc_id = d.doc_id
   AND d.doc_id = dcr.doc_id
  AND (:bvCategoryId IS NULL OR
       dcr.category_id IN
       (SELECT c.category_id
                 FROM cms_category_t c
                WHERE c.attribute5 LIKE '%' ||
                      (SELECT attribute5
                         FROM cms_category_t
                        WHERE category_id = :bvCategoryId) || '%') OR dcr.category_id = :bvCategoryId)]]></SQLQuery>
