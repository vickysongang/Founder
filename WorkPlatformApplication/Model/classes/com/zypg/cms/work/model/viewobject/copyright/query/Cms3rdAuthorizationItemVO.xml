  <SQLQuery><![CDATA[SELECT item.item_id,
       book.book_name,
       book.isbn,
       book.author,
       third.Authorized_Company,
       third.order_name,
       third.order_id,
       item.order_doc_id,
       item.author_doc_id,
       third.authorized_start_time,
       add_months(third.authorized_start_time,
                  to_number(substr(third.authorized_valid_period,
                                   instr(third.authorized_valid_period, '_') + 1))) authorized_end_time,
       item.exclusive_copyright_flag,
       exclusive_copyright_lookup.meaning exclusive_copyright_display,
       item.sales_total_cost,
       item.sales_total_num,
       company.company_name,
       months_between(add_months(third.authorized_start_time,
                                 to_number(substr(third.authorized_valid_period,
                                                  instr(third.authorized_valid_period,
                                                        '_') + 1))),
                      SYSDATE) expire_months
  FROM cms_author_copyright_t        author,
       cms_3rd_authorization_order_t third,
       cms_3rd_authorization_item_t  item,
       cms_lookup_value_v            exclusive_copyright_lookup,
       cms_company_copyright_t       company,
       cms_book_t                    book
 WHERE author.doc_id = item.author_doc_id
   AND third.authorized_company = company.company_id
   AND item.order_doc_id = third.doc_id
   AND book.doc_id = author.book_doc_id
   AND item.exclusive_copyright_flag =
       exclusive_copyright_lookup.lookup_value_code
   AND exclusive_copyright_lookup.lookup_type_code = 'YESORNO']]></SQLQuery>
