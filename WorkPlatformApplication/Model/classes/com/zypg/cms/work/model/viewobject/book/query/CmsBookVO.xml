  <SQLQuery><![CDATA[SELECT b.book_id,
       b.doc_id,
       b.book_name,
       b.isbn,
       b.pub_time,
       b.edition || '-' || b.impression edition_impression,
       b.author,
       b.edition,
       b.impression,
       decode(sign(b.last_update_date - d.last_update_date),
              -1,
              d.last_update_date,
              b.last_update_date) last_update_date,
       d.status,
       d.lib_code,
       dcr.category_id,
       dcr.category_group_type,
       nvl(d.delete_flag, 'N') delete_flag,
       NULL checked,
       d.thumbnail_url,
       b.keyword keyword,
       d.comp_code,
       b.publishing_house,
       d.chapter_flag,
       d.attribute1 sync_flag,
       b.series,
       c.category_name,
       d.attribute2, --最近一次发布时间
       b.item_code,
       b.lang,
       b.editor,
       b.format_design,
       b.cover_design,
       b.chief_editor,
       b.translator,
       b.pagination,
       b.price,
       b.reader_group,
       b.suggestion,
       b.prod_size,
       b.color_num,
       b.cover_typeset,
       b.assort_prod,
       b.remarks,
       b.book_size,
       b.main_page,
       b.main_typeset,
       b.format_comp
  FROM cms_book_t             b,
       cms_doc_t              d,
       cms_doc_category_rel_t dcr,
       cms_category_t         c
 WHERE b.doc_id = d.doc_id
   AND d.doc_id = dcr.doc_id
   AND dcr.category_id = c.category_id(+)
   AND (:bvCategoryId IS NULL OR
       dcr.category_id IN
       (SELECT c.category_id
           FROM cms_category_t c
          WHERE c.attribute5 LIKE
                '%' || (SELECT attribute5
                          FROM cms_category_t
                         WHERE category_id = :bvCategoryId) || '%') OR
       dcr.category_id = :bvCategoryId)]]></SQLQuery>
