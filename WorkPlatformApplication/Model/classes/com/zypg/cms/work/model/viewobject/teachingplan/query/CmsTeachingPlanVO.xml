  <SQLQuery><![CDATA[SELECT t.teaching_plan_id,
       t.doc_id,
       t.teaching_plan_name,
       t.source_book_name,
       t.producer,
       t.publishing_house,
       t.teaching_plan_store_time,
       decode(sign(t.last_update_date - d.last_update_date),
              -1,
              d.last_update_date,
              t.last_update_date) last_update_date,
       d.STATUS,
       d.lib_code,
       d.comp_code,
       NULL checked,
       dcr.category_id,
       dcr.category_group_type,
       NVL(d.delete_flag, 'N') delete_flag,
       t.source_book_isbn source_book_isbn,
       t.keyword keyword,
       t.teaching_plan_type teaching_plan_type,
       d.attribute1 sync_flag,
       t.editor,
       t.copyright_flag,
       t.remarks
  FROM cms_teaching_plan_t t, cms_doc_t d, cms_doc_category_rel_t dcr
 WHERE t.doc_id = d.doc_id
   AND d.doc_id = dcr.doc_id
   AND (:bvCategoryId IS NULL OR
       dcr.category_id IN
       (SELECT c.category_id
           FROM cms_category_t c
          WHERE c.attribute5 LIKE
                '%' || (SELECT attribute5
                          FROM cms_category_t
                         WHERE category_id = :bvCategoryId) || '%') OR
       dcr.category_id = :bvCategoryId)]]></SQLQuery>
