  <SQLQuery><![CDATA[SELECT CmsTaskMonitorTEO.TASK_ID,
       CmsTaskMonitorTEO.Doc_Id doc_id,
       CmsTaskMonitorTEO.TASK_NAME,
       CmsTaskMonitorTEO.TASK_SPONSOR,
       sponsor.diplay_name TASK_SPONSOR_NAME,
       CmsTaskMonitorTEO.CURR_OPERATOR,
       curr_operator.diplay_name CURR_OPERATOR_NAME,
       d.STATUS,
       dsv.status_name,
       CmsTaskMonitorTEO.DESCRIPTION,
       CmsTaskMonitorTEO.CREATED_BY,
       CmsTaskMonitorTEO.CREATION_DATE,
       decode(sign(CmsTaskMonitorTEO.last_update_date - d.last_update_date),
              -1,
              d.last_update_date,
              CmsTaskMonitorTEO.last_update_date) last_update_date,
       d.lib_code,
       d.Comp_Code,
       NULL checked
  FROM CMS_TASK_MONITOR_T CmsTaskMonitorTEO,
       cms_doc_t          d,
       cms_user_t         sponsor,
       cms_user_t         curr_operator,
       cms_doc_status_v   dsv
 WHERE CmsTaskMonitorTEO.Doc_Id = d.doc_id
   AND d.lib_code = 'OFFLINE_ENTRY_PROCESS'
   AND CmsTaskMonitorTEO.TASK_SPONSOR = sponsor.user_id(+)
   AND CmsTaskMonitorTEO.CURR_OPERATOR = curr_operator.user_id(+)
   AND dsv.lib_code = d.lib_code
   AND d.Status = dsv.status_code]]></SQLQuery>
