  <SQLQuery><![CDATA[SELECT t.illustration_id,
       t.doc_id,
       t.illustration_name,
       t.illustration_source,
       decode(sign(t.last_update_date - d.last_update_date),
              -1,
              d.last_update_date,
              t.last_update_date) last_update_date,
       d.status,
       d.lib_code,
       d.comp_code,
       NULL checked,
       dcr.category_id,
       dcr.category_group_type,
       nvl(d.delete_flag, 'N') delete_flag,
       d.thumbnail_url,
       t.illustration_author,
       t.page_number,
       t.keyword,
       t.illustration_desc,
       t.remarks,
       t.figure_format,
       t.figure_dimension,
       t.figure_size,
       t.cover_design,
       t.figure_resolution,
       t.figure_x_resolution,
       t.figure_y_resolution,
       t.figure_width,
       t.figure_height,
       t.figure_store_time,
       t.copyright_flag
  FROM cms_illustration_t t, cms_doc_t d, cms_doc_category_rel_t dcr
 WHERE t.doc_id = d.doc_id
   AND d.doc_id = dcr.doc_id
   AND (:bvCategoryId IS NULL OR
       dcr.category_id IN
       (SELECT c.category_id
           FROM cms_category_t c
          WHERE c.attribute5 LIKE
                '%' || (SELECT attribute5
                          FROM cms_category_t
                         WHERE category_id = :bvCategoryId) || '%') OR
       dcr.category_id = :bvCategoryId)]]></SQLQuery>
