       CmsPhotographyFigureTEO.REMARKS,
       CmsPhotographyFigureTEO.FIGURE_FORMAT,
       CmsPhotographyFigureTEO.FIGURE_SIZE,
       CmsPhotographyFigureTEO.FIGURE_DIMENSION,
       CmsPhotographyFigureTEO.FIGURE_RESOLUTION,
       CmsPhotographyFigureTEO.FIGURE_X_RESOLUTION,
       CmsPhotographyFigureTEO.FIGURE_Y_RESOLUTION,
       CmsPhotographyFigureTEO.COVER_DESIGN,
       NULL checked,
       dcr.category_id,
       c.category_name,
       CmsPhotographyFigureTEO.Figure_Width,
       CmsPhotographyFigureTEO.Figure_Height,
       d.status,
       d.comp_code,
       comp.comp_name
  FROM CMS_PHOTOGRAPHY_FIGURE_T CmsPhotographyFigureTEO,
       cms_doc_category_rel_t   dcr,
       cms_category_t           c,
       cms_doc_t d,
       cms_comp_v             comp
 WHERE CmsPhotographyFigureTEO.doc_id = dcr.doc_id(+)
   AND dcr.category_group_type(+) = 'BY_CATEGORY'
   AND dcr.category_id = c.category_id(+)
   AND CmsPhotographyFigureTEO.Doc_Id = d.doc_id
   and  d.status = 'PUBED_PHOTOGRAPHY_FIGURE'
   and d.comp_code = comp.comp_code
   and d.comp_code = decode(:bvGroupUserFlag,'Y',d.comp_code,:bvCompCode)]]></SQLQuery>
