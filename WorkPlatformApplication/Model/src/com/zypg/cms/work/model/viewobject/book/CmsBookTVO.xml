       cmsbookteo.lang,
       cmsbookteo.last_update_date,
       cmsbookteo.last_updated_by,
       cmsbookteo.main_page,
       cmsbookteo.main_typeset,
       cmsbookteo.object_version_number,
       cmsbookteo.pagination,
       cmsbookteo.price,
       cmsbookteo.prod_size,
       cmsbookteo.pub_time,
       cmsbookteo.reader_group,
       cmsbookteo.remarks,
       cmsbookteo.series,
       cmsbookteo.suggestion,
       cmsbookteo.translator,
       cmsbookteo.book_name,
       dcr.category_id,
       c.category_name,
       cmsbookteo.publishing_house
  FROM cms_book_t cmsbookteo, cms_doc_category_rel_t dcr,cms_category_t c
 WHERE cmsbookteo.doc_id = dcr.doc_id(+)
   AND dcr.category_group_type(+) = 'BY_CATEGORY'
   AND dcr.category_id = c.category_id(+)]]></SQLQuery>
