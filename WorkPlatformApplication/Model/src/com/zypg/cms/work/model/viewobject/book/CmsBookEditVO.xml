  <SQLQuery><![CDATA[SELECT NULL assort_prod,--配套产品
       NULL author,--作者
       NULL book_id,
       NULL book_size,--开本
       NULL chief_editor,--主编
       NULL color_num,--正文色数
       NULL cover_design,--封面设计
       NULL cover_typeset,--封面排版软件
       NULL doc_id,
       NULL edition,--版次
       NULL editor,--责任编辑
       NULL format_comp,--排版公司
       NULL format_design,--版面设计
       NULL impression,--印次
       NULL isbn,
       NULL item_code,--物料号
       NULL keyword,--关键词
       NULL lang,--语种
       NULL main_page,--正文用纸
       NULL main_typeset,--正文排版软件
       NULL pagination,--页数
       NULL price, --定价
       NULL prod_size,--成品尺寸
       NULL pub_time,--出版时间
       NULL reader_group,--读者对象
       NULL remarks,--备注
       NULL series,--丛书名
       NULL suggestion,--上架建议
       NULL translator,--译者
       NULL book_name, --书名
       NULL category_id,
       NULL category_name, --图书分类
       NULL publishing_house--出版社
  FROM dual]]></SQLQuery>
