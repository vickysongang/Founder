  <SQLQuery><![CDATA[SELECT DISTINCT act.copyright_id,
                act.doc_id,
                act.network_copyright_flag,
                act.network_end_time,
                act.audio_video_copyright_flag,
                act.audio_video_end_time,
                act.broadcast_copyright_flag,
                act.broadcast_end_time,
                act.paper_medium_copyright_flag,
                act.paper_medium_end_time,
                act.electronic_copyright_flag,
                act.electronic_end_time,
                act.authorized_area,
                act.authorized_lang,
                d.status,
                d.comp_code,
                d.lib_code,
                b.book_name,
                b.isbn,
                b.author,
                act.exclusive_copyright_flag,
                exclusive_copyright_lookup.meaning exclusive_copyright_display,
                act.public_copyright_flag,
                public_copyright_lookup.meaning public_copyright_display,
                act.copyright_form,
                copyright_form.meaning copyright_form_display,
                NULL checked,
                dcr.category_id,
                dcr.category_group_type,
                nvl(d.delete_flag, 'N') delete_flag,
                ct.category_name,
                decode(null,
                       'PAPER',
                       months_between(act.paper_medium_end_time, SYSDATE),
                       'ELEC',
                       months_between(act.electronic_end_time, SYSDATE),
                       'MEDIA',
                       months_between(act.audio_video_end_time, SYSDATE),
                       'NETWORK',
                       months_between(act.network_end_time, SYSDATE),
                       'BROADCAST',
                       months_between(act.broadcast_end_time, SYSDATE),
                       null) months,
                b.edition, /*版次*/
                act.price, /*纸书定价*/
                act.copyright_agent, /*版权代理人*/
                act.copyright_owner, /*著作权人*/
                act.copyright_registration_number, /*版权登记号*/
                act.digital_royalty, /*数字版税*/
                act.overseas_royalty, /*海外版税*/
                b.pub_time, /*出版时间*/
                b.impression, /*印次*/
                act.attribute1, /*信息网络传播权*/
                act.attribute2, /*信息网络第三方转授权*/
                act.e_book_min_price, /*电子书最低定价*/
                act.e_book_price, /*电子书定价*/
                act.part_probation_flag, /*部分试读*/
                act.overseas_copyright_flag, /*海外版权*/
                act.remarks /*备注*/
  FROM cms_author_copyright_t act,
       cms_book_t             b,
       cms_doc_t              d,
       cms_doc_category_rel_t dcr,
       cms_doc_category_rel_t authorcategory,
       cms_category_t         ct,
       cms_lookup_value_v     exclusive_copyright_lookup,
       cms_lookup_value_v     public_copyright_lookup,
       cms_lookup_value_v     copyright_form
 WHERE act.doc_id = d.doc_id
   AND act.book_doc_id = b.doc_id
   AND b.doc_id = dcr.doc_id
   AND ct.category_id(+) = dcr.category_id
   AND act.doc_id = authorcategory.doc_id
   AND act.exclusive_copyright_flag =
       exclusive_copyright_lookup.lookup_value_code(+)
   AND exclusive_copyright_lookup.lookup_type_code = 'YOUORWU'
   AND act.public_copyright_flag =
       public_copyright_lookup.lookup_value_code(+)
   AND public_copyright_lookup.lookup_type_code = 'YOUORWU'
   AND act.copyright_form = copyright_form.lookup_value_code(+)
   AND copyright_form.lookup_type_code = 'COPYRIGHT_FORM'
   AND (:bvCategoryId IS NULL OR
       authorcategory.category_id IN
       (SELECT c.category_id
           FROM cms_category_t c
          START WITH c.category_id = :bvCategoryId
         CONNECT BY PRIOR c.parent_node_id = c.category_id
                AND c.parent_node_type = 'CATEGORY_GROUP'
                AND  cms_work_platform_pkg.get_category_lib_code(c.category_id) = 'AUTHOR') OR
       authorcategory.category_id = :bvCategoryId)
       AND decode(:bvCopyrightType,
              'NETWORK',
              act.network_copyright_flag,
              'PAPER',
              act.paper_medium_copyright_flag,
              'ELEC',
              act.electronic_copyright_flag,
              'MEDIA',
              act.audio_video_copyright_flag,
              'BROADCAST',
              act.broadcast_copyright_flag,'YOU') = 'YOU']]></SQLQuery>
