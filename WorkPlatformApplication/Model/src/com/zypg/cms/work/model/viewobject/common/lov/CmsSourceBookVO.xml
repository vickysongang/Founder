  <SQLQuery><![CDATA[SELECT b.book_id,
       b.doc_id,
       b.book_name,
       b.isbn,
       b.pub_time,
       b.edition || '-' || b.impression edition_impression,
       b.author,
       b.edition,
　　　 b.series,
       b.impression,
       b.last_update_date,
       d.status,
       d.lib_code,
       dcr.category_id,
       dcr.category_group_type,
       nvl(d.delete_flag, 'N') delete_flag,
       NULL checked,
       d.thumbnail_url,
       b.keyword keyword,
       d.comp_code,
       b.publishing_house,
       b.editor,
       dct.category_name,
       (b.book_name||','||b.isbn||','||b.edition||','||b.impression) display_name
  FROM cms_book_t b, cms_doc_t d, cms_doc_category_rel_t dcr,cms_category_t dct
 WHERE b.doc_id = d.doc_id
   AND d.doc_id = dcr.doc_id
   AND dcr.category_id = dct.category_id(+)
   AND d.comp_code = :bvCompCode]]></SQLQuery>
