  <SQLQuery><![CDATA[SELECT f.figure_id,
       f.figure_name,
       f.figure_dimension,
       f.doc_id,
       f.photographer,
       f.keyword,
       f.last_update_date,
       d.status,
       d.lib_code,
       d.comp_code,
       dcr.category_id,
       dcr.category_group_type,
       nvl(d.delete_flag, 'N') delete_flag,
       NULL checked,
       d.thumbnail_url,
       'N' group_flag,
       NULL figure_count,
       NULL group_id,
       'N' group_detail_flag,
       f.photography_time,
       f.photography_type,
       f.series_name,
       f.photography_location,
       f.remarks,
       f.figure_desc,
       f.figure_format,
       f.figure_resolution,
       f.figure_size,
       f.figure_x_resolution,
       f.figure_y_resolution,
       f.cover_design,
       f.figure_width,
       f.figure_height
  FROM cms_photography_figure_t f, cms_doc_t d, cms_doc_category_rel_t dcr
 WHERE f.doc_id = d.doc_id
   AND d.doc_id = dcr.doc_id
   AND f.doc_id NOT IN
       (SELECT rt.doc_id
          FROM cms_doc_group_rel_t rt, cms_figure_group_t g
         WHERE rt.group_id = g.group_id
           AND g.lib_code = 'PHOTOGRAPHY_FIGURE')
   AND (:bvCategoryId IS NULL OR
       dcr.category_id IN
       (SELECT c.category_id
           FROM cms_category_t c
          START WITH c.category_id = :bvCategoryId
         CONNECT BY PRIOR c.category_id = c.parent_node_id
                AND c.parent_node_type = 'CATEGORY') OR
       dcr.category_id = :bvCategoryId)
UNION ALL
SELECT f.figure_id,
       g.group_name figure_name,
       f.figure_dimension,
       f.doc_id,
       f.photographer,
       f.keyword,
       f.last_update_date,
       d.status,
       d.lib_code,
       d.comp_code,
       dcr.category_id,
       dcr.category_group_type,
       nvl(d.delete_flag, 'N') delete_flag,
       NULL checked,
       d.thumbnail_url,
       'Y' group_flag,
       (SELECT COUNT(*)
          FROM cms_doc_group_rel_t rt
         WHERE rt.group_id = dgr.group_id) figure_count,
       g.group_id,
       'N' group_detail_flag,
       f.photography_time,
       f.photography_type,
       f.series_name,
       f.photography_location,
       f.remarks,
       f.figure_desc,
       f.figure_format,
       f.figure_resolution,
       f.figure_size,
       f.figure_x_resolution,
       f.figure_y_resolution,
       f.cover_design,
       f.figure_width,
       f.figure_height
  FROM cms_photography_figure_t f,
       cms_doc_t                d,
       cms_doc_category_rel_t   dcr,
       cms_doc_group_rel_t      dgr,
       cms_figure_group_t       g
 WHERE f.doc_id = d.doc_id
   AND d.doc_id = dcr.doc_id
   AND d.doc_id = dgr.doc_id
   AND dgr.index_flag = 'Y'
   AND dgr.group_id = g.group_id
   AND g.lib_code = 'PHOTOGRAPHY_FIGURE'
   AND (:bvCategoryId IS NULL OR
       dcr.category_id IN
       (SELECT c.category_id
           FROM cms_category_t c
          START WITH c.category_id = :bvCategoryId
         CONNECT BY PRIOR c.category_id = c.parent_node_id
                AND c.parent_node_type = 'CATEGORY') OR
       dcr.category_id = :bvCategoryId)
UNION ALL
SELECT f.figure_id,
       f.figure_name,
       f.figure_dimension,
       f.doc_id,
       f.photographer,
       f.keyword,
       decode(sign(f.last_update_date - d.last_update_date),
              -1,
              d.last_update_date,
              f.last_update_date) last_update_date,
       d.status,
       d.lib_code,
       d.comp_code,
       dcr.category_id,
       dcr.category_group_type,
       nvl(d.delete_flag, 'N') delete_flag,
       NULL checked,
       d.thumbnail_url,
       'N' group_flag,
       NULL figure_count,
       g.group_id,
       'Y' group_detail_flag,
       f.photography_time,
       f.photography_type,
       f.series_name,
       f.photography_location,
       f.remarks,
       f.figure_desc,
       f.figure_format,
       f.figure_resolution,
       f.figure_size,
       f.figure_x_resolution,
       f.figure_y_resolution,
       f.cover_design,
       f.figure_width,
       f.figure_height
  FROM cms_photography_figure_t f,
       cms_doc_t                d,
       cms_doc_category_rel_t   dcr,
       cms_doc_group_rel_t      dgr,
       cms_figure_group_t       g
 WHERE f.doc_id = d.doc_id
   AND d.doc_id = dcr.doc_id
   AND d.doc_id = dgr.doc_id
   AND dgr.group_id = g.group_id
   AND g.lib_code = 'PHOTOGRAPHY_FIGURE'
   AND (:bvCategoryId IS NULL OR
       dcr.category_id IN
       (SELECT c.category_id
           FROM cms_category_t c
          WHERE c.attribute5 LIKE
                '%' || (SELECT attribute5
                          FROM cms_category_t
                         WHERE category_id = :bvCategoryId) || '%') OR
       dcr.category_id = :bvCategoryId)]]></SQLQuery>
