  <SQLQuery><![CDATA[SELECT CmsVideoTEO.VIDEO_ID,
       CmsVideoTEO.FILE_NAME,
       CmsVideoTEO.SERIES_NAME,
       CmsVideoTEO.ISBN,
       CmsVideoTEO.ITEM_CODE,
       CmsVideoTEO.VIDEO_TYPE,
       CmsVideoTEO.CARRIER_TYPE,
       CmsVideoTEO.LANG,
       CmsVideoTEO.EDITOR,
       CmsVideoTEO.TEXT_EDITOR,
       CmsVideoTEO.PRODUCER,
       CmsVideoTEO.PUB_TIME,
       CmsVideoTEO.ASSORT_BOOK,
       CmsVideoTEO.KEYWORD,
       CmsVideoTEO.CONTENT_DESC,
       CmsVideoTEO.REMARKS,
       CmsVideoTEO.DOC_ID,
       CmsVideoTEO.FILE_FORMAT,
       CmsVideoTEO.FILE_SIZE,
       CmsVideoTEO.FILE_TIME,
       CmsVideoTEO.FILE_STORE_TIME,
       NULL checked,
       dcr.category_id,
       c.category_name,
       d.status,
       d.thumbnail_url,
       d.comp_code,
       comp.comp_name
  FROM CMS_VIDEO_T            CmsVideoTEO,
       cms_doc_category_rel_t dcr,
       cms_category_t         c,
       cms_doc_t              d,
       cms_comp_v             comp
 WHERE CmsVideoTEO.doc_id = dcr.doc_id(+)
   AND dcr.category_group_type(+) = 'BY_CATEGORY'
   AND dcr.category_id = c.category_id(+)
   AND CmsVideoTEO.doc_id = d.doc_id
   and d.status = 'PUBED_VIDEO'
   and d.comp_code = comp.comp_code
   and d.comp_code = decode(:bvGroupUserFlag,'Y',d.comp_code,:bvCompCode)]]></SQLQuery>
