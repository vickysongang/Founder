  <SQLQuery><![CDATA[SELECT b.doc_id,
       b.book_name,
       b.isbn,
       b.edition,
       b.impression,
       b.editor,
       b.author,
       b.pub_time,
       b.series,
       nvl(c.paper_medium_copyright_flag, 'WU') paper_medium_copyright_flag,
       nvl(c.network_copyright_flag, 'WU') network_copyright_flag,
       d.comp_code,
       b.price,
       ct.category_id,
       ct.category_name,
       c.paper_medium_start_time,
       c.paper_medium_end_time,
       c.network_start_time,
       c.network_end_time,
       d.status,
       comp.comp_name,
       cms_common_pkg.get_category_top_node_id(ct.category_id)
  FROM cms_book_t               b,
       cms_doc_t                d,
       cms_author_copyright_t   c,
       cms_doc_category_rel_t   crt,
       cms_category_t           ct,
       cms_comp_v               comp
 WHERE b.doc_id = d.doc_id
   AND d.doc_id = crt.doc_id(+)
   AND crt.category_id = ct.category_id(+)
   AND d.lib_code = 'BOOK'
   AND d.doc_id = c.book_doc_id(+)
   AND d.comp_code = comp.comp_code]]></SQLQuery>
