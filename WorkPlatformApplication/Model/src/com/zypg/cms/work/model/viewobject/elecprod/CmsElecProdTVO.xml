  <SQLQuery><![CDATA[SELECT cmselecprodteo.elec_prod_id,
       cmselecprodteo.elec_prod_name,
       cmselecprodteo.isbn,
       cmselecprodteo.item_code,
       cmselecprodteo.assort_book,
       cmselecprodteo.prod_type,
       cmselecprodteo.publishing_house,
       cmselecprodteo.lang,
       cmselecprodteo.carrier_type,
