       cmselecprodteo.creation_date,
       cmselecprodteo.last_updated_by,
       cmselecprodteo.last_update_date,
       cmselecprodteo.doc_id,
       dcr.category_id,
       c.category_name,
       cmselecprodteo.script,
       d.comp_code,
       comp.comp_name
  FROM cms_elec_prod_t        cmselecprodteo,
       cms_doc_t              d,
       cms_doc_category_rel_t dcr,
       cms_category_t         c,
       cms_comp_v             comp
 WHERE cmselecprodteo.doc_id = dcr.doc_id(+)
   AND cmselecprodteo.doc_id = d.doc_id
   AND dcr.category_group_type(+) = 'BY_CATEGORY'
   AND dcr.category_id = c.category_id(+)
   AND d.comp_code = comp.comp_code]]></SQLQuery>
