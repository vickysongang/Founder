  <SQLQuery><![CDATA[SELECT p.periodical_id,
       p.doc_id,
       p.periodical_category_id,
       p.editor,
       p.period_id,
       p.format_design,
       p.cover_design,
       p.pub_time,
       p.keyword,
       p.prod_size,
       p.color_num,
       p.cover_typeset,
       p.assort_prod,
       p.book_size,
       p.main_typeset,
       p.main_page,
       p.pagination,
       p.suggestion,
       p.remarks,
       d.status,
       d.lib_code,
       d.comp_code,
       NVL(d.DELETE_FLAG, 'N') delete_flag,
       d.THUMBNAIL_URL,
       to_char(ps.YEAR) YEAR,
       ps.period_num,
       v.periodical_category_name,
       v.periodical_type,
       v.total,
       d.attribute1 sync_flag
  FROM cms_periodical_t          p,
       cms_doc_t                 d,
       cms_period_setup_t        ps,
       cms_periodical_category_v v
 WHERE p.doc_id = d.doc_id
   AND p.period_id = ps.setup_id(+)
   AND p.periodical_category_id = v.periodical_category_id(+)]]></SQLQuery>
