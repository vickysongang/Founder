       CmsWorksEntryTEO.WORD_COUNT,
       CmsWorksEntryTEO.WRITING_TIME,
       CmsWorksEntryTEO.STORE_TIME,
       CmsWorksEntryTEO.KEYWORD,
       CmsWorksEntryTEO.ABSTARCT_DESC,
       NULL checked,
       dcr.category_id,
       c.category_name,
       d.status,
       d.comp_code,
       comp.comp_name
  FROM CMS_WORKS_ENTRY_T      CmsWorksEntryTEO,
       cms_doc_category_rel_t dcr,
       cms_category_t         c,
       cms_doc_t              d,
       cms_comp_v             comp
 WHERE CmsWorksEntryTEO.doc_id = dcr.doc_id(+)
   AND dcr.category_group_type(+) = 'BY_CATEGORY'
   AND dcr.category_id = c.category_id(+)
   AND CmsWorksEntryTEO.Doc_Id = d.doc_id
   and d.status ='PUBED_WORKS_ENTRY'
   and d.comp_code = comp.comp_code
   and d.comp_code = decode(:bvGroupUserFlag,'Y',d.comp_code,:bvCompCode)]]></SQLQuery>
