<?xml version='1.0' encoding='UTF-8'?>
<ui:composition xmlns:ui="http://java.sun.com/jsf/facelets" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"
                xmlns:f="http://java.sun.com/jsf/core">
    <af:panelStretchLayout id="psl1" inlineStyle="border:1px solid #BAC6CA;background:#F3F8FC;" topHeight="auto"
                           bottomHeight="0px">
        <f:facet name="top">
            <af:panelStretchLayout id="psl4" inlineStyle="height:66px;" startWidth="auto" endWidth="auto"
                                   styleClass="AFStretchWidth" bottomHeight="auto">
                <f:facet name="start">
                    <af:panelGroupLayout id="pgl1"
                                         partialTriggers="cb1 cil6 cil3 cil8 cil13 cb11 cil2 cil4 d5 d1 it1 soc1 soc2 d2 t1:sbc1 t1:sbc2"
                                         inlineStyle="margin:5px 5px 5px 15px;padding-left:20px;" layout="horizontal"
                                         binding="#{backingBeanScope.photographyMainManagedBean.buttonPanelGroupLayout}">
                        <af:commandImageLink text="新建" id="cb1" partialSubmit="true" action="toEdit" useWindow="true"
                                             windowHeight="500" windowWidth="650" icon="/resources/images/new.gif"
                                             styleClass="imagelink_length_2"
                                             visible="#{backingBeanScope.photographyMainManagedBean.commonRenderer.createEnable}"
                                             returnListener="#{backingBeanScope.photographyMainManagedBean.commonRenderer.createOrEditReturnListener}">
                            <af:setPropertyListener from="CREATE" to="#{pageFlowScope.mode}" type="action"/>
                            <af:setPropertyListener from="N" to="#{pageFlowScope.batchFlag}" type="action"/>
                        </af:commandImageLink>
                        <af:commandImageLink text="提交" id="cb3" icon="/resources/images/submit.gif"
                                             styleClass="imagelink_length_2"
                                             action="#{backingBeanScope.photographyMainManagedBean.refreshCoverAction}"
                                             visible="#{backingBeanScope.photographyMainManagedBean.commonRenderer.submitEnable and backingBeanScope.photographyMainManagedBean.commonRenderer.currSelectedListSize >= 1}"
                                             actionListener="#{backingBeanScope.photographyMainManagedBean.commonRenderer.updateDocStatusActionListener}">
                            <f:attribute name="operation" value="提交"/>
                        </af:commandImageLink>
                        <af:commandImageLink text="审核" id="cil18" icon="/resources/images/edit.gif" windowHeight="560"
                                             windowWidth="650" partialSubmit="true" action="toEdit" useWindow="true"
                                             styleClass="imagelink_length_2"
                                             returnListener="#{backingBeanScope.photographyMainManagedBean.commonRenderer.createOrEditReturnListener}"
                                             visible="#{backingBeanScope.photographyMainManagedBean.commonRenderer.approveEnable  and backingBeanScope.photographyMainManagedBean.commonRenderer.currSelectedListSize == 1}"
                                             actionListener="#{backingBeanScope.photographyMainManagedBean.commonRenderer.docIdSetActionListener}">
                            <af:setPropertyListener from="APPROVE" to="#{pageFlowScope.mode}" type="action"/>
                            <af:setPropertyListener from="N" to="#{pageFlowScope.batchFlag}" type="action"/>
                        </af:commandImageLink>
                        <af:commandImageLink text="标引" id="cil14" icon="/resources/images/edit.gif" windowHeight="560"
                                             windowWidth="650" partialSubmit="true" action="toEdit" useWindow="true"
                                             styleClass="imagelink_length_2"
                                             returnListener="#{backingBeanScope.photographyMainManagedBean.commonRenderer.createOrEditReturnListener}"
                                             visible="#{backingBeanScope.photographyMainManagedBean.commonRenderer.indexEnable and backingBeanScope.photographyMainManagedBean.commonRenderer.currSelectedListSize == 1  and pageFlowScope.selectedGroupFlag !='Y'}"
                                             actionListener="#{backingBeanScope.photographyMainManagedBean.commonRenderer.docIdSetActionListener}">
                            <af:setPropertyListener from="EDIT" to="#{pageFlowScope.mode}" type="action"/>
                            <af:setPropertyListener from="N" to="#{pageFlowScope.batchFlag}" type="action"/>
                        </af:commandImageLink>
                        <af:commandImageLink text="标引" id="cil1" icon="/resources/images/edit.gif" windowHeight="560"
                                             windowWidth="680" partialSubmit="true" action="toEditFigureGroup"
                                             useWindow="true" styleClass="imagelink_length_2"
                                             visible="#{backingBeanScope.photographyMainManagedBean.commonRenderer.indexEnable and backingBeanScope.photographyMainManagedBean.commonRenderer.currSelectedListSize == 1  and pageFlowScope.selectedGroupFlag =='Y'}"
                                             actionListener="#{backingBeanScope.photographyMainManagedBean.commonRenderer.docIdSetActionListener}">
                            <af:setPropertyListener from="EDIT" to="#{pageFlowScope.mode}" type="action"/>
                            <af:setPropertyListener from="N" to="#{pageFlowScope.batchFlag}" type="action"/>
                        </af:commandImageLink>
                        <af:commandImageLink text="批量标引" id="cil6" icon="/resources/images/batch_edit.gif"
                                             windowHeight="460" windowWidth="650" partialSubmit="true" action="toEdit"
                                             useWindow="true" styleClass="imagelink_length_4"
                                             returnListener="#{backingBeanScope.photographyMainManagedBean.commonRenderer.createOrEditReturnListener}"
                                             actionListener="#{backingBeanScope.photographyMainManagedBean.commonRenderer.docIdBatchSetActionListener}"
                                             visible="#{backingBeanScope.photographyMainManagedBean.commonRenderer.batchIndexEnable  and backingBeanScope.photographyMainManagedBean.commonRenderer.currSelectedListSize > 1}">
                            <af:setPropertyListener from="EDIT" to="#{pageFlowScope.mode}" type="action"/>
                            <af:setPropertyListener from="Y" to="#{pageFlowScope.batchFlag}" type="action"/>
                        </af:commandImageLink>
                        <af:commandImageLink text="批量采集" id="cil20" icon="/resources/images/file_manager.gif"
                                             styleClass="imagelink_length_4"
                                             action="#{backingBeanScope.photographyMainManagedBean.refreshCoverAction}"
                                             visible="#{backingBeanScope.photographyMainManagedBean.commonRenderer.batchFgmtEnable}"
                                             actionListener="#{backingBeanScope.photographyMainManagedBean.commonRenderer.batchFileManageActionListener}"></af:commandImageLink>
                        <af:commandImageLink text="批量同步" id="cil21" icon="/resources/images/file_manager.gif"
                                             styleClass="imagelink_length_4"
                                             action="#{backingBeanScope.photographyMainManagedBean.refreshCoverAction}"
                                             visible="#{backingBeanScope.photographyMainManagedBean.commonRenderer.batchSyncEnable}"
                                             actionListener="#{backingBeanScope.photographyMainManagedBean.commonRenderer.batchSyncActionListener}"></af:commandImageLink>
                        <af:commandImageLink text="设置关联" id="cil16" icon="/resources/images/relation.gif"
                                             windowHeight="600" windowWidth="800" partialSubmit="true" useWindow="true"
                                             action="toRelationSet" styleClass="imagelink_length_4"
                                             actionListener="#{backingBeanScope.photographyMainManagedBean.commonRenderer.docIdSetActionListener}"
                                             visible="#{backingBeanScope.photographyMainManagedBean.commonRenderer.relationEnable and backingBeanScope.photographyMainManagedBean.commonRenderer.currSelectedListSize == 1}"/>
                        <af:commandImageLink text="附件管理" id="cil12" icon="/resources/images/edit.gif" windowHeight="560"
                                             windowWidth="650" partialSubmit="true" action="toAttach" useWindow="true"
                                             styleClass="imagelink_length_4"
                                             visible="#{backingBeanScope.photographyMainManagedBean.commonRenderer.attachmentMgmtEnable  and backingBeanScope.photographyMainManagedBean.commonRenderer.currSelectedListSize == 1}"
                                             actionListener="#{backingBeanScope.photographyMainManagedBean.commonRenderer.docIdSetActionListener}"></af:commandImageLink>
                        <af:commandImageLink text="替换" id="cil3" icon="/resources/images/replace.gif" windowHeight="560"
                                             windowWidth="650" partialSubmit="true" action="toAttach" useWindow="true"
                                             styleClass="imagelink_length_2"
                                             visible="#{backingBeanScope.photographyMainManagedBean.commonRenderer.changeImgEnable  and backingBeanScope.photographyMainManagedBean.commonRenderer.currSelectedListSize == 1}"
                                             actionListener="#{backingBeanScope.photographyMainManagedBean.commonRenderer.docIdSetActionListener}"></af:commandImageLink>
                        <af:commandImageLink text="新建组图" id="cil8" icon="/resources/images/new_group.gif"
                                             windowHeight="580" windowWidth="700" partialSubmit="true"
                                             action="toNewFigureGroup" useWindow="true" styleClass="imagelink_length_4"
                                             visible="#{backingBeanScope.photographyMainManagedBean.commonRenderer.createGroupEnable and backingBeanScope.photographyMainManagedBean.commonRenderer.currSelectedListSize >= 1}"
                                             actionListener="#{backingBeanScope.photographyMainManagedBean.commonRenderer.docIdBatchSetActionListener}"></af:commandImageLink>
                        <af:commandImageLink text="加入组图" id="cil13" icon="/resources/images/group_add.gif"
                                             windowHeight="460" windowWidth="650" partialSubmit="true"
                                             action="toJoinFigureGroup" useWindow="true" styleClass="imagelink_length_4"
                                             visible="#{backingBeanScope.photographyMainManagedBean.commonRenderer.addToGroupEnable and backingBeanScope.photographyMainManagedBean.commonRenderer.currSelectedListSize == 1}"
                                             actionListener="#{backingBeanScope.photographyMainManagedBean.commonRenderer.docIdBatchSetActionListener}"></af:commandImageLink>
                        <af:commandImageLink text="拆分组图" id="cil7" icon="/resources/images/group_delete.gif"
                                             styleClass="imagelink_length_4"
                                             visible="#{backingBeanScope.photographyMainManagedBean.commonRenderer.splitGroupEnable  and backingBeanScope.photographyMainManagedBean.commonRenderer.currSelectedListSize == 1}"
                                             actionListener="#{backingBeanScope.photographyMainManagedBean.deleteFigureGroupActionListener}"></af:commandImageLink>
                        <af:commandImageLink text="发布" id="cb5" icon="/resources/images/pub.gif"
                                             styleClass="imagelink_length_2"
                                             action="#{backingBeanScope.photographyMainManagedBean.refreshCoverAction}"
                                             visible="#{backingBeanScope.photographyMainManagedBean.commonRenderer.pubEnable and backingBeanScope.photographyMainManagedBean.commonRenderer.currSelectedListSize >= 1}"
                                             actionListener="#{backingBeanScope.photographyMainManagedBean.commonRenderer.updateDocStatusActionListener}">
                            <f:attribute name="operation" value="提交"/>
                        </af:commandImageLink>
                        <af:commandImageLink text="删除" id="cb6" icon="/resources/images/delete.gif"
                                             styleClass="imagelink_length_2" partialSubmit="true"
                                             visible="#{backingBeanScope.photographyMainManagedBean.commonRenderer.deleteEnable and backingBeanScope.photographyMainManagedBean.commonRenderer.currSelectedListSize >= 1}">
                            <af:showPopupBehavior popupId="p3" triggerType="action"/>
                        </af:commandImageLink>
                        <af:commandImageLink text="还原" id="cb7" icon="/resources/images/recycle.gif"
                                             styleClass="imagelink_length_2"
                                             action="#{backingBeanScope.photographyMainManagedBean.refreshCoverAction}"
                                             rendered="#{backingBeanScope.photographyMainManagedBean.commonRenderer.recoverEnable and backingBeanScope.photographyMainManagedBean.commonRenderer.currSelectedListSize >= 1}"
                                             actionListener="#{backingBeanScope.photographyMainManagedBean.commonRenderer.recoverDocActionListener}"/>
                        <af:commandImageLink text="彻底删除" id="cil10" icon="/resources/images/delete_final.gif"
                                             styleClass="imagelink_length_4" partialSubmit="true"
                                             visible="#{backingBeanScope.photographyMainManagedBean.commonRenderer.destroyEnable and backingBeanScope.photographyMainManagedBean.commonRenderer.currSelectedListSize >= 1}">
                            <af:showPopupBehavior popupId="p1" triggerType="action"/>
                        </af:commandImageLink>
                        <af:commandImageLink text="回退" id="cb11" icon="/resources/images/recycle.gif"
                                             styleClass="imagelink_length_2" partialSubmit="true"
                                             visible="#{backingBeanScope.photographyMainManagedBean.commonRenderer.backEnable and backingBeanScope.photographyMainManagedBean.commonRenderer.currSelectedListSize >= 1}"
                                             actionListener="#{backingBeanScope.photographyMainManagedBean.commonRenderer.docIdBatchSetActionListener}"
                                             action="toRollback" useWindow="true" windowHeight="200" windowWidth="620"
                                             returnListener="#{backingBeanScope.photographyMainManagedBean.rollbackReturnListener}">
                            <af:setPropertyListener to="#{pageFlowScope.type}" type="action" from="ROLLBACK"/>
                        </af:commandImageLink>
                        <af:commandImageLink text="撤销发布" id="cil2" icon="/resources/images/uppub.gif"
                                             styleClass="imagelink_length_4" partialSubmit="true"
                                             visible="#{backingBeanScope.photographyMainManagedBean.commonRenderer.undoPubEnable and backingBeanScope.photographyMainManagedBean.commonRenderer.currSelectedListSize >= 1}"
                                             actionListener="#{backingBeanScope.photographyMainManagedBean.commonRenderer.docIdBatchSetActionListener}"
                                             action="toRollback" useWindow="true" windowHeight="130" windowWidth="250"
                                             returnListener="#{backingBeanScope.photographyMainManagedBean.rollbackReturnListener}">
                            <af:setPropertyListener to="#{pageFlowScope.type}" type="action" from="CANCEL"/>
                        </af:commandImageLink>
                        <af:commandImageLink text="操作日志" id="cil4" icon="/resources/images/log.gif"
                                             styleClass="imagelink_length_4"
                                             visible="#{backingBeanScope.photographyMainManagedBean.commonRenderer.currSelectedListSize == 1 and pageFlowScope.deleteFlag == 'N'}"
                                             action="toProcessLog"
                                             actionListener="#{backingBeanScope.photographyMainManagedBean.commonRenderer.docIdSetActionListener}"
                                             useWindow="true" partialSubmit="true" windowHeight="400"
                                             windowWidth="600"/>
                        <af:commandImageLink text="回退记录" id="cil5" icon="/resources/images/catlog_view.gif"
                                             styleClass="imagelink_length_4" partialSubmit="true"
                                             visible="#{backingBeanScope.photographyMainManagedBean.commonRenderer.currSelectedListSize == 1 and pageFlowScope.deleteFlag == 'N'}"
                                             actionListener="#{backingBeanScope.photographyMainManagedBean.commonRenderer.docIdSetActionListener}"
                                             action="toRollbackLog" useWindow="true" windowHeight="400"
                                             windowWidth="600"/>
                        <af:commandImageLink text="添加到暂存架" id="cil19" icon="/resources/images/catlog_view.gif"
                                             styleClass="imagelink_length_6" partialSubmit="true"
                                             visible="#{backingBeanScope.photographyMainManagedBean.commonRenderer.addToTempShelfEnable and backingBeanScope.photographyMainManagedBean.commonRenderer.currSelectedListSize >= 1}"
                                             actionListener="#{backingBeanScope.photographyMainManagedBean.commonRenderer.addToTempShelf}"/>
                        <af:commandImageLink text="查看批量同步日志" id="cil15" icon="/resources/images/catlog_view.gif"
                                             styleClass="imagelink_length_6" useWindow="true" partialSubmit="true"
                                             windowHeight="400" windowWidth="1200"
                                             visible="#{backingBeanScope.photographyMainManagedBean.commonRenderer.batchSyncLogEnable and pageFlowScope.deleteFlag == 'N'}"
                                             action="toBatchSyncLog"/>
                        <af:popup childCreation="deferred" autoCancel="disabled" id="p3"
                                  binding="#{backingBeanScope.photographyMainManagedBean.commonRenderer.deletePopup}">
                            <af:dialog id="d5" title="提示"
                                       dialogListener="#{backingBeanScope.photographyMainManagedBean.deleteDocDialogActionListener}">
                                <af:outputText value="你确定要删除吗?" id="ot12"/>
                            </af:dialog>
                        </af:popup>
                        <af:popup childCreation="deferred" autoCancel="disabled" id="p1"
                                  binding="#{backingBeanScope.photographyMainManagedBean.commonRenderer.destoryPopup}">
                            <af:dialog id="d1" title="提示"
                                       dialogListener="#{backingBeanScope.photographyMainManagedBean.destroyDocActionListener}">
                                <af:outputText value="你确定要彻底删除吗?" id="ot14"/>
                            </af:dialog>
                        </af:popup>
                    </af:panelGroupLayout>
                </f:facet>
                <f:facet name="end">
                    <af:panelGroupLayout layout="horizontal" id="pgl9" inlineStyle="margin-top:10px;margin-right:15px;">
                        <af:inputText label="searchInput" id="it1" simple="true"
                                      shortDesc="输入关键字,可以对以下内容[摄影人,图片名称,关键词]进行快速检索"
                                      contentStyle="width:150px;height:18px;line-height:18px;background-image:none;border-color:#ABADB3;"
                                      binding="#{backingBeanScope.photographyMainManagedBean.commonRenderer.searchInput}">
                            <af:clientListener type="keyPress" method="queryWhenPressEnter"/>
                            <af:serverListener type="queryWhenPressAction"
                                               method="#{backingBeanScope.photographyMainManagedBean.queryWhenPressEnterListener}"/>
                        </af:inputText>
                        <af:commandImageLink text="" id="cil9" icon="/resources/images/search.gif"
                                             inlineStyle="margin-left:2px;"
                                             actionListener="#{backingBeanScope.photographyMainManagedBean.searchActionListenser}"/>
                        <af:commandImageLink text="" id="cil25" icon="/resources/images/icon_hzddsh.gif"
                                             partialSubmit="true" inlineStyle="margin-left:5px;"
                                             action="toComplexSearch" useWindow="true" windowHeight="500"
                                             windowWidth="650" shortDesc="高级搜索"
                                             returnListener="#{backingBeanScope.photographyMainManagedBean.complexRearchReturnListener}"/>
                    </af:panelGroupLayout>
                </f:facet>
                <f:facet name="bottom">
                    <af:panelStretchLayout id="psl3" inlineStyle="height:26px;background:#E7ECE5;margin-top:3px;"
                                           topHeight="0px" bottomHeight="0px" startWidth="auto"
                                           styleClass="AFStretchWidth"
                                           partialTriggers="cl1 cl2 soc1 soc2 cb1 cil2 d5 d1 cil8 cil13 it1 cb11 cil25"
                                           endWidth="auto">
                        <f:facet name="bottom"/>
                        <f:facet name="center"/>
                        <f:facet name="start">
                            <af:panelGroupLayout id="pgl11" layout="horizontal"
                                                 inlineStyle="padding-top:3px;padding-left:15px;">
                                <af:outputText value="总个数:#{backingBeanScope.photographyMainManagedBean.tableModel.rowCount}"
                                               id="ot2"/>
                                <af:spacer width="10" height="10" id="s2"/>
                                <af:outputText value="每页[20]" id="ot8"/>
                                <af:spacer width="10" height="10" id="s7"/>
                                <af:outputText value="页数[" id="ot9"/>
                                <af:outputText value="#{backingBeanScope.photographyMainManagedBean.tableModel.currPageIndex}"
                                               id="ot6" inlineStyle="color:Red;"/>
                                <af:outputText value="/#{backingBeanScope.photographyMainManagedBean.tableModel.totalPageCount}]"
                                               id="ot4"/>
                                <af:spacer width="10" height="10" id="s4"/>
                                <af:commandLink text="前一页" id="cl1"
                                                disabled="#{backingBeanScope.photographyMainManagedBean.tableModel.isPrevDisabled}"
                                                action="#{backingBeanScope.photographyMainManagedBean.refreshCoverAction}"
                                                actionListener="#{backingBeanScope.photographyMainManagedBean.commonRenderer.prevPage}"/>
                                <af:spacer width="10" height="10" id="s1"/>
                                <af:commandLink text="后一页" id="cl2"
                                                disabled="#{backingBeanScope.photographyMainManagedBean.tableModel.isNextDisabled}"
                                                action="#{backingBeanScope.photographyMainManagedBean.refreshCoverAction}"
                                                actionListener="#{backingBeanScope.photographyMainManagedBean.commonRenderer.nextPage}"/>
                                <af:spacer width="10" height="10" id="s3"/>
                                <af:selectOneChoice valueChangeListener="#{backingBeanScope.photographyMainManagedBean.goPageIndex}"
                                                    id="soc1" simple="true" autoSubmit="true" shortDesc=" "
                                                    value="#{backingBeanScope.photographyMainManagedBean.tableModel.currItem.value}">
                                    <f:selectItems value="#{backingBeanScope.photographyMainManagedBean.tableModel.pageSelectItems}"
                                                   id="si1"/>
                                </af:selectOneChoice>
                                <af:spacer width="10" height="10" id="s6"/>
                                <af:commandImageLink id="cl3" icon="/resources/images/shuaxin.gif" shortDesc="刷新"
                                                     actionListener="#{backingBeanScope.photographyMainManagedBean.refreshActionListener}"/>
                            </af:panelGroupLayout>
                        </f:facet>
                        <f:facet name="end">
                            <af:panelGroupLayout layout="horizontal" id="pgl7" partialTriggers="cl4"
                                                 inlineStyle="margin-right:40px;padding-top:3px;">
                                <af:forEach items="#{backingBeanScope.photographyMainManagedBean.commonRenderer.statusMapList}"
                                            var="status">
                                    <af:panelGroupLayout layout="horizontal" id="pgl13"
                                                         styleClass="#{pageFlowScope.currStatusCode == status['statusCode'] ? 'statusSelected' : 'none'}">
                                        <af:commandLink text="#{status['displayName']}" id="cl5"
                                                        visible="#{pageFlowScope.deleteFlag != 'Y'}"
                                                        actionListener="#{backingBeanScope.photographyMainManagedBean.switchStatusActionListener}">
                                            <f:attribute name="statusCode" value="#{status['statusCode']}"/>
                                            <af:setPropertyListener to="#{pageFlowScope.currStatusCode}" type="action"
                                                                    from="#{status['statusCode']}"/>
                                        </af:commandLink>
                                    </af:panelGroupLayout>
                                    <af:spacer width="5" height="10" id="s11"/>
                                </af:forEach>
                                <af:selectOneChoice value="#{pageFlowScope.currStatusCode}" autoSubmit="true" id="soc2"
                                                    shortDesc=" " visible="#{pageFlowScope.deleteFlag != 'Y'}"
                                                    rendered="false"
                                                    valueChangeListener="#{backingBeanScope.photographyMainManagedBean.commonRenderer.statusValueChangeListener}">
                                    <f:selectItems value="#{backingBeanScope.photographyMainManagedBean.commonRenderer.statusList}"
                                                   id="si2"/>
                                </af:selectOneChoice>
                                <af:spacer width="15" height="10" id="s10"/>
                                <af:forEach items="#{backingBeanScope.photographyMainManagedBean.commonRenderer.docDisplayWayMapList}"
                                            var="displayWay">
                                    <af:panelGroupLayout layout="horizontal" id="pgl14"
                                                         styleClass="#{pageFlowScope.defaultDisplayWay == displayWay['statusCode'] ? 'statusSelected' : 'none'}">
                                        <af:commandLink text="#{displayWay['statusName']}" id="cl6"
                                                        actionListener="#{backingBeanScope.photographyMainManagedBean.commonRenderer.switchDisplayWayActionListenter}">
                                            <f:attribute name="statusCode" value="#{displayWay['statusCode']}"/>
                                        </af:commandLink>
                                    </af:panelGroupLayout>
                                    <af:spacer width="5" height="10" id="s12"/>
                                </af:forEach>
                                <af:selectOneChoice autoSubmit="true" value="#{pageFlowScope.defaultDisplayWay}"
                                                    id="soc3" rendered="false"
                                                    valueChangeListener="#{backingBeanScope.photographyMainManagedBean.commonRenderer.displayWayValueChangeListener}">
                                    <f:selectItems value="#{backingBeanScope.photographyMainManagedBean.commonRenderer.docDisplayWayList}"
                                                   id="si3"/>
                                </af:selectOneChoice>
                                <af:spacer width="10" height="10" id="s8"/>
                                <af:commandImageLink text="" id="cl4" shortDesc="返回" partialSubmit="true"
                                                     visible="#{pageFlowScope.groupFlag == 'Y'}"
                                                     actionListener="#{backingBeanScope.photographyMainManagedBean.returnActionListener}"
                                                     binding="#{backingBeanScope.photographyMainManagedBean.returnLink}"
                                                     icon="/resources/images/up.gif"/>
                            </af:panelGroupLayout>
                        </f:facet>
                    </af:panelStretchLayout>
                </f:facet>
            </af:panelStretchLayout>
        </f:facet>
        <f:facet name="bottom">
            <af:panelGroupLayout id="pgl10">
                <af:popup childCreation="deferred" autoCancel="disabled" id="p2"
                          binding="#{backingBeanScope.photographyMainManagedBean.deleteFigureGroupPopup}">
                    <af:dialog id="d2" title="提示"
                               dialogListener="#{backingBeanScope.photographyMainManagedBean.deleteFigureGroupDialogListener}">
                        <f:facet name="buttonBar"/>
                        <af:panelGroupLayout id="pgl6" layout="horizontal">
                            <af:outputText value="确定拆分组图？" id="ot3"/>
                        </af:panelGroupLayout>
                    </af:dialog>
                </af:popup>
            </af:panelGroupLayout>
        </f:facet>
        <f:facet name="center">
            <af:panelStretchLayout id="psl2" topHeight="auto" dimensionsFrom="children"
                                   partialTriggers="cb1 cil14 cil6 cil3 cil8 cil13 cil2 d5 d1 it1 soc1 cl4 d2 cil11 soc3"
                                   binding="#{backingBeanScope.photographyMainManagedBean.coverPanelStretchLayout}">
                <f:facet name="center">
                    <af:switcher id="s5" defaultFacet="COVER_LIST" facetName="#{pageFlowScope.defaultDisplayWay}">
                        <f:facet name="COVER_LIST">
                            <af:panelGroupLayout id="pgl2" inlineStyle="border:1px solid #F1F1F1;" layout="scroll"
                                                 binding="#{backingBeanScope.photographyMainManagedBean.commonRenderer.coverPanelGroupLayout}">
                                <af:forEach items="#{backingBeanScope.photographyMainManagedBean.photographyList}"
                                            var="photographyList" varStatus="listVs">
                                    <af:panelGroupLayout id="pgl15" layout="vertical" clientComponent="true">
                                        <table width="100%" xmlns="http://www.w3.org/1999/xhtml">
                                            <tr>
                                                <af:forEach items="#{photographyList}" var="photography"
                                                            varStatus="prodVs">
                                                    <td width="20%">
                                                        <af:panelGroupLayout id="pgl3" layout="vertical" halign="center"
                                                                             styleClass="coverListCell"
                                                                             visible="#{photography['figureId'] == null ? 'false' : 'true'}"
                                                                             inlineStyle="background:#{photography['checked'] ? '#D6E5EB':'white'};">
                                                            <af:panelGroupLayout id="pgl8" layout="horizontal"
                                                                                 halign="center"
                                                                                 styleClass="coverStyle">
                                                                <af:commandImageLink id="cil11" partialSubmit="true"
                                                                                     clientComponent="true"
                                                                                     styleClass="#{photography['thumbnailUrl']== null ? 'noCoverImage' : 'coverImage'}"
                                                                                     icon="#{photography['thumbnailUrl']== null ? '/resources/images/lack_image.gif' : photography['thumbnailUrl']}">
                                                                    <af:setPropertyListener from="EDIT"
                                                                                            to="#{pageFlowScope.mode}"
                                                                                            type="action"/>
                                                                    <f:attribute name="checked"
                                                                                 value="#{photography['checked']== null ? true : !photography['checked']}"/>
                                                                    <f:attribute name="docId"
                                                                                 value="#{photography['docId']}"/>
                                                                    <f:attribute name="groupId"
                                                                                 value="#{photography['groupId']}"/>
                                                                    <f:attribute name="figureCount"
                                                                                 value="#{photography['figureCount']}"/>
                                                                    <af:setPropertyListener from="#{photography['groupFlag']}"
                                                                                            to="#{pageFlowScope.selectedGroupFlag}"
                                                                                            type="action"/>
